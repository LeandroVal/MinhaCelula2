@using MinhaCelula.Model
@model IEnumerable<Usuario>
@{
                /**/

                Layout = null;

}

@foreach (Usuario Us in Model)
{
<tr id="@Us.UsuarioId">
    <th scope="row">
        <i class="material-icons">delete</i>
        <i class="material-icons">edit</i>
    </th>
    <th scope="row">@Us.UsuarioId</th>
    <td>@Us.UserName</td>
    <td>@Us.Email</td>
    <td>@Us.Status</td>
</tr>

}


